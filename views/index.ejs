      <!-- Calculate CGPA -->
      <section id="calculateCgpa" class="three">
        <div class="container">
          <header>
            <h2>CGPA Calculator</h2>
          </header>
          <form action="#">
            <div class="col-12">
              <article class="item">
                <label for="semCompleted"
                  >Please, Choose number of semester's you have completed till
                  now</label
                ><br />
                <div class="myContainer">
                  <select
                    name="semCompleted"
                    id="semCompleted"
                    required
                    oninvalid="this.setCustomValidity('Please Select Semester Nanba')"
                    oninput="this.setCustomValidity('')"
                  >
                    <option value="" selected disabled hidden>
                      Semesters Completed
                    </option>
                    <option value="1">Semester 1</option>
                    <option value="2">Semester 2</option>
                    <option value="3">Semester 3</option>
                    <option value="4">Semester 4</option>
                    <option value="5">Semester 5</option>
                    <option value="6">Semester 6</option>
                    <option value="7">Semester 7</option>
                    <option value="8">Semester 8</option>
                  </select>
                  <!-- Go Button -->
                  <button type="submit" id="semCompletedBtn">GO!</button>
                </div>
              </article>
            </form>
            <form action="#">
              <!-- Dynamic List Container -->
              <div class="semListContainer">
                <div class="col-12-mobile" id="semList"></div>
              </div>
              <input
                class="myButton cgpaCalcBtn"
                id="cgpaBtn"
                type="submit"
                value="CALCULATE"
              />
            </form>
              <!-- Result Box -->
              <div id="resultView">
                <label id="resultLabel" for="inputResultView"
                  >Your CGPA is</label
                >
                <input id="result" class="resultBox" type="text" readonly />
              </div>
            </div>
 
        </div>
      </section>
      
